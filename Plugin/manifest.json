{
    "manifest_version": 3,
  
    "name": "Duckling",
    "description": "Life goes on and on and on!",
    "version": "1.0",
  
    "action": {
     "default_icon": "/static/icon.png",
     "default_popup": "popup.html"
    },

    "options_page": "options.html",
    
    "background": {
        "service_worker": "background.js"
    },

    "permissions": [
     "storage",
     "activeTab",
     "scripting"
     ]
  }